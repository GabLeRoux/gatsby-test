    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "npm run lint:js -- --fix",
    "format:styles": "npm run lint:styles -- --fix",
    "storybook": "storybook dev -p 6006 -s static",
    "analyze": "cross-env APP_ANALYZE=true npm run build"
  },
